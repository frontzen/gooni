{
  "name": "@front.zen/react",
  "version": "1.0.0",
  "license": "MIT",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "yarn run -T vite build --mode=development --watch",
    "build": "yarn run -T vite build && yarn build:copy",
    "build:copy": "node ../../scripts/copyFiles.mjs",
    "test": "yarn run -T vitest --run",
    "type-check": "yarn run -T tsc"
  },
  "exports": {
    ".": {
      "default": "./src"
    }
  },
  "module": "./src/index.ts",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "devDependencies": {
    "@front.zen/utils": "^1.0.0"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "dependencies": {
    "@react-stately/utils": "^3.2.1"
  }
}
